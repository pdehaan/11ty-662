{
  "name": "11ty-662",
  "description": "How do I build multiple sites from a single install without folder permalinks?",
  "version": "1.0.0",
  "author": "Peter deHaan <peter@deseloper.com> (https://about.me/peterdehaan)",
  "bugs": {
    "url": "https://github.com/pdehaan/11ty-662/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "npm-run-all": "^4.1.5"
  },
  "homepage": "https://github.com/pdehaan/11ty-662#readme",
  "keywords": [],
  "license": "MPL-2.0",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdehaan/11ty-662.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:site-1": "eleventy --config .eleventy.site-1.js",
    "build:site-2": "eleventy --config .eleventy.site-2.js",
    "build:site-3": "eleventy --input=./src/site-3 --output=./dist/site-3",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
